{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "migrate:win": "(if exist dev.db del dev.db) && (if not exist dev.db sqlite3 dev.db \"VACUUM;\") && sqlite3 dev.db < src/lib/migrations/init.sql",
    "migrate:unix": "(test -f dev.db && rm dev.db || echo 'No existing DB') && (test -f dev.db || sqlite3 dev.db \"VACUUM;\") && sqlite3 dev.db < src/lib/migrations/init.sql",
    "migrate": "npm run migrate:win || npm run migrate:unix",
    "migrate:postgres": "ts-node src/lib/migrations/migrate.ts",
    "seed:postgres": "ts-node src/lib/migrations/seed.ts",
    "reset:postgres": "ts-node src/lib/migrations/reset.ts",
    "build": "DISABLE_ESLINT_PLUGIN=true next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@langchain/core": "^0.3.43",
    "@langchain/google-genai": "^0.2.0",
    "@types/leaflet": "^1.9.17",
    "@types/pg": "^8.11.13",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "d3-geo": "^3.1.1",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "langchain": "^0.3.19",
    "leaflet": "^1.9.4",
    "leaflet-geosearch": "^4.2.0",
    "leaflet-providers": "^2.0.0",
    "mysql2": "^3.12.0",
    "next": "^15.2.4",
    "nodemailer": "^6.10.0",
    "pg": "^8.14.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-leaflet": "^5.0.0",
    "react-router-dom": "^7.2.0",
    "resend": "^4.1.2",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^3.0.1",
    "topojson": "^3.0.2"
